<%= simple_form_for(@education) do |f| %>
<%= f.input :title, collection: ["Doctor", "Professor"] %>
<%= f.input :university %>
<%= f.input :degree %>
<%= f.label :score %>
<%= f.range_field :score, in: 50..100 %>
<%= f.submit %>
<% end %>

